program satu;
{progaram ini akan menampilkan huruf yang ditentukan dengan angka}
var
       angka: integer;

begin
        readln(angka);
        if (angka>70) then
                writeln('A');
        if (angka>60) then
                writeln('B');
        if (angka>50) then
                writeln('C');
        writeln('selesai');
end.
